{
  "name": "react-stubber",
  "version": "1.0.0",
  "description": "Simple but useful stubbing solution React",
  "repository": {
    "type": "git",
    "url": "https://github.com/kadirahq/react-stubber"
  },
  "license": "MIT",
  "scripts": {
    "prepublish": "rimraf dist && cross-env NODE_ENV=production babel --ignore tests,stories --plugins transform-runtime src --out-dir dist",    
    "lint": "eslint src",
    "lintfix": "eslint src --fix",
    "testonly": "mocha --require .scripts/mocha_runner src/**/tests/**/*.js",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "storybook": "start-storybook -p 9010",
    "publish-storybook": "bash .scripts/publish_storybook.sh"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-cli": "^6.26.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.1",
    "eslint": "^4.10.0",
    "babel-eslint": "^7.2.3",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "enzyme": "^3.1.0",
    "git-url-parse": "^7.0.1",
    "mocha": "^4.0.1",
    "jsdom": "^11.3.0",
    "react-addons-test-utils": "^15.6.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "sinon": "^4.1.0",
    "style-loader": "^0.19.0"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  },
  "main": "dist/index.js",
  "engines": {
    "npm": "^3.0.0"
  }
}
